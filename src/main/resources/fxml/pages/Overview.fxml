<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="overviewContentPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.phicaro.tseks.ui.controller.OverviewController">
   <center>
      <SplitPane id="events_splitpane" dividerPositions="0.66" pickOnBounds="false" style="-fx-background-color: #ffffff;" stylesheets="@../styles/Styles.css" BorderPane.alignment="CENTER">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="158.0" prefWidth="135.0">
               <children>
                  <VBox layoutX="212.0" layoutY="88.0" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <TableView fx:id="eventTable" VBox.vgrow="ALWAYS">
                          <columns>
                            <TableColumn fx:id="eventTableNameColumn" prefWidth="75.0" />
                            <TableColumn fx:id="eventTableDateColumn" prefWidth="75.0" />
                              <TableColumn fx:id="eventTableLocationColumn" prefWidth="75.0" />
                              <TableColumn fx:id="eventTableOptionsColumn" prefWidth="75.0" />
                          </columns>
                           <VBox.margin>
                              <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                           </VBox.margin>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <HBox>
                           <children>
                              <Pane HBox.hgrow="ALWAYS" />
                              <Button fx:id="addEventButton" alignment="CENTER_RIGHT" mnemonicParsing="false" styleClass="primary" text="Button" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="30.0" left="15.0" right="15.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" stylesheets="@../styles/Styles.css">
               <children>
                  <VBox id="info_vbox" fx:id="infoPane" layoutX="18.0" layoutY="13.0" styleClass="text-area" stylesheets="@../styles/Styles.css" AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0">
                     <children>
                        <Label fx:id="infoEventTitle" style="-fx-font-size: 14px;" text="Label">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin></Label>
                        <VBox styleClass="text-area">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                           <children>
                              <Label fx:id="infoEventDesc" text="Label">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </Label>
                              <TableView fx:id="infoEventTable">
                                 <columns>
                                    <TableColumn fx:id="infoTableCountColumn" prefWidth="75.0" text="" />
                                    <TableColumn fx:id="infoTableSeatsColumn" prefWidth="75.0" text="" />
                                    <TableColumn fx:id="infoTablePriceColumn" prefWidth="75.0" text="" />
                                 </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </VBox>
                        <VBox alignment="BOTTOM_CENTER" VBox.vgrow="ALWAYS">
                           <children>
                              <Button fx:id="printButton" mnemonicParsing="false" styleClass="success" text="Button" />
                           </children>
                           <VBox.margin>
                              <Insets bottom="15.0" top="30.0" />
                           </VBox.margin>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                     </padding></VBox>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </center>
</BorderPane>
