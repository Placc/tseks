<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<BorderPane fx:id="content" style="-fx-background-color: #cccccc;" styleClass="root" stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.phicaro.tseks.ui.controller.MainController">
    <top>
        <HBox fx:id="toolbar" minHeight="40.0" styleClass="tool-bar" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER_LEFT" styleClass="headers-region" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="pageTitle" style="-fx-font-size: 24px;" styleClass="hea" text="Label" />
               </children>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin></HBox>
            <MenuButton fx:id="optionButton" alignment="CENTER" mnemonicParsing="false" styleClass="arrow-button">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </MenuButton>
         </children>
      </HBox>
    </top>
</BorderPane>
